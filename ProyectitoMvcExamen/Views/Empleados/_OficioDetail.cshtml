@model List<Empleado>

    <h1>Partial de empleados por oficio</h1>

    <table class="table table-info">
    <thead>
        <tr>
            <th>Id</th>
            <th>Apellido</th>
            <th>Oficio</th>
            <th>Salario</th>
            <th>Id Departamento</th>
        </tr>

    </thead>

    <tbody>
        @foreach (Empleado emp in Model)
        {
                <tr>
                    <td>@emp.Emp_no</td>
                    <td>@emp.Apellido</td>
                    <td>@emp.Oficio</td>
                    <td>@emp.Salario</td>
                    <td>@emp.Dept_no</td>
                </tr>
        }
    </tbody>
</table>
